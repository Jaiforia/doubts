#include<stdio.h>
#include<conio.h>

void main()
{
  clrsce();
  char ch,x[100];
  int i,count=0;
  printf("Enter the string:\n");
  gets(x);
  printf("Enter the letter:\n");
  scanf("%c",&ch);
   for (i=0;x[i]!='\0';i++)
    {
      if(x[i]==ch)
      {
        count++;
      }
    }
    printf("\n%d",&count);
  getch();
}
